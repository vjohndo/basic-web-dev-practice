<%- include("header"); -%>
    <div class="box" id="heading">
        <h1> <%= listTitle %> </h1>
    </div>
    
    <div class="box">
        <% for (item of toDoList) { %>
        <div class="item">
            <input type="checkbox">
            <p><%= item %></p>
        </div>
        <% } %>
        <form class="item" action="/" method="POST">
            <input type="text" name="item" placeholder="New Item" autocomplete="off">
            <button type="submit" name="list" value="<%= listTitle %>">+</button>
        </form>
    </div>
<%- include("footer"); -%>